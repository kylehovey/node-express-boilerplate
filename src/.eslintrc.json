{
    "env": {
        "browser": true,
        "es6": true,
        "jquery" : true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars" : "off",
        "no-undef" : "off"
    }
}
